{
  "name": "rssfeeds",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "astro dev",
    "build:astro": "astro build",
    "build:pagefind": "npx pagefind",
    "static:pagefind": "npx shx cp -r dist/pagefind public/",
    "build": "npm-run-all -s build:astro build:pagefind static:pagefind",
    "serve:pagefind": "npx pagefind --serve",
    "dev:astro": "astro dev",
    "dev": "npm-run-all -s build:pagefind static:pagefind dev:astro",
    "dev:build": "npm-run-all -s build dev",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "is-ci || husky",
    "commitlint": "commitlint --edit",
    "genuuid": "node -e \"console.log(require('uuid').v7())\"",
    "add-feed": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/addFeed.ts"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.1.0",
    "@fontsource-variable/montserrat": "^5.1.1",
    "@fontsource-variable/rubik": "^5.1.1",
    "@material/web": "^2.2.0",
    "@murtuzaalisurti/back-to-top": "^3.0.4",
    "@netlify/functions": "^3.0.0",
    "astro": "^5.1.9",
    "consola": "^3.4.0",
    "dayjs": "^1.11.13",
    "is-ci": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.10",
    "@types/prompts": "^2.4.9",
    "@types/uuid": "^10.0.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "pagefind": "^1.3.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "uuid": "^11.0.5"
  }
}
